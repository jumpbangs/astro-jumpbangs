{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "components/*": ["src/components/*"],
      "common/*": ["src/components/common/*"],
      "styles/*": ["src/styles/*"],
      "layouts/*": ["src/layouts/*"],
      "utils/*": ["src/utils/*"],
      "assets/*": ["src/assets/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "include": ["**/*.ts", "**/*.tsx", "**/*.astro"]
}
